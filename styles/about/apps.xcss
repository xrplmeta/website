.apps{
	.listings{
		margin-top: 15px;
		gap: 32px;

		> flex{
			gap: 20px;

			> icon{
				width: 100px;
				height: 100px;
				margin-top: 4px;
			}
		}


		link icon{
			width: 16px;
			color: var(--color-hint);
			margin-left: 5px;
		}

		label.name{
			font-weight: bold;
			font-size: 21px;
		}

		label.desc{
			font-size: 14px;
			color: var(--color-hint);
		}
	}
}