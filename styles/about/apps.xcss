.apps{
	.listings{
		margin-top: 15px;
		gap: 45px;

		stack > icon{
			width: 100px;
			height: 100px;
			margin-bottom: 10px;
		}

		flex > icon{
			color: var(--color-hint);
			margin-left: 5px;
		}

		label.name{
			font-weight: bold;
		}

		label.desc{
			font-size: 14px;
			color: var(--color-hint);
		}
	}
}