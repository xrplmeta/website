const buttonCount = 5

export default x => {
	let page = x.attrs.page + 1
	let startPage = 1

	return (
		<flex class="pager">
			{#for let i=startPage; i<startPage+buttonCount; i++}
				<button 
					class={i === page ? `active` : ``}
					onclick={() => x.attrs.onpage(i)}
				>
					{i}
				</button>
			{/for}
		</flex>
	)
}